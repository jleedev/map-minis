            "exponential",
            2
          ],
          [
            "zoom"
          ],
          8,
          2,
          16,
          [
            "case",
            [
              "==",
              [
                "get",
                "brunnel"
              ],
              "bridge"
            ],
            14,
            8
          ]
        ],
        "line-blur": 0.5,
        "line-color": "#cccccc"
