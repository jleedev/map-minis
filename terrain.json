    "openmaptiles": {
      "type": "vector",
      "url": "https://tile.ourmap.us/data/v3.json"
    },
    "dem": {
      "type": "raster-dem",
      "attribution": "<a target=\"_blank\" rel=\"noopener\" href=\"https://registry.opendata.aws/terrain-tiles/\">Terrain Tiles</a>",
      "tiles": [
        "https://s3.amazonaws.com/elevation-tiles-prod/terrarium/{z}/{x}/{y}.png"
      ],
      "encoding": "terrarium",
      "tileSize": 256,
      "maxzoom": 15
    },
    "base": {
      "type": "vector",
      "attribution": "<a href=\"https://overturemaps.org\" rel=\"noopener\" target=\"_blank\">Overture Maps Foundation</a>",
      "url": "pmtiles://https://d3c1b7bog2u1nn.cloudfront.net/2024-09-18/base.pmtiles"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "hsl(30, 44%, 96%)"
      }
    },
    {
      "id": "base",
      "type": "fill",
      "source": "base",
      "source-layer": "land_cover",
      "filter": [
        "has",
        "subtype"
      ],
      "paint": {
        "fill-color": [
          "interpolate",
          [
            "exponential",
            1.2
          ],
          [
            "zoom"
          ],
          4,
          [
            "match",
            [
              "get",
              "subtype"
            ],
            "snow",
            "rgba(224, 255, 255, 0.3)",
            "forest",
            "rgba(144, 238, 144, 0.2)",
            "crop",
            "rgba(250, 250, 210, 0.2)",
            "grass",
            "rgba(124, 252, 0, 0.1)",
            "shrub",
            "rgba(216, 191, 116, 0.1)",
            "barren",
            "rgba(253, 245, 230, 0.2)",
            "urban",
            "rgba(240, 230, 240, 0.4)",
            "moss",
            "rgba(0, 100, 0, 0.1)",
            "wetland",
            "rgba(32, 178, 170, 0.2)",
            "mangrove",
            "rgba(46, 139, 87, 0.2)",
            "magenta"
          ],
          17,
          [
            "match",
            [
              "get",
              "subtype"
            ],
            "snow",
            "rgba(224, 255, 255, 0.2)",
            "forest",
            "rgba(144, 238, 144, 0)",
            "crop",
            "rgba(250, 250, 210, 0)",
            "grass",
            "rgba(124, 252, 0, 0)",
            "shrub",
            "rgba(216, 191, 116, 0)",
            "barren",
            "rgba(253, 245, 230, 0.1)",
            "urban",
            "rgba(240, 230, 240, 0.4)",
            "moss",
            "rgba(0, 100, 0, 0)",
            "wetland",
            "rgba(32, 178, 170, 0)",
            "mangrove",
            "rgba(46, 139, 87, 0)",
            "magenta"
          ]
        ]
      }
    },
    {
      "id": "hillshading",
      "type": "hillshade",
      "source": "dem",
      "paint": {
        "hillshade-exaggeration": [
          "interpolate",
          [
            "linear"
          ],
          [
            "zoom"
          ],
          12,
          0.5,
          18,
          0.1
        ],
        "hillshade-shadow-color": "hsla(30, 14%, 76%, 1)",
        "hillshade-highlight-color": "hsla(30, 44%, 99%, 1)"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "openmaptiles",
      "source-layer": "water",
      "paint": {
