            "exponential",
            2
          ],
          [
            "zoom"
          ],
          8,
          1,
          16,
          6
        ],
        "line-color": [
          "case",
          [
            "==",
            [
              "get",
              "brunnel"
            ],
            "tunnel"
          ],
          "#eeeeee",
          "#ffffff"
        ]
      }
    },
    {
      "id": "boundary",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "boundary",
      "filter": [
        "all",
        [
          "has",
          "admin_level"
        ],
        [
          "<=",
          [
            "get",
            "admin_level"
          ],
          7
        ],
        [
          "!=",
          [
            "get",
            "maritime"
          ],
          1
        ]
      ],
      "paint": {
        "line-color": "hsl(330, 44%, 86%)",
        "line-blur": [
          "/",
