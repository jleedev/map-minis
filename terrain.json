    },
    {
      "id": "waterway",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "waterway",
      "paint": {
        "line-color": "hsl(211, 50%, 85%)",
        "line-width": [
          "interpolate",
          [
            "exponential",
            2
          ],
          [
            "zoom"
          ],
          3,
          0.5,
          16,
          10
        ]
      }
    },
    {
      "id": "highway-casing",
      "type": "line",
      "source": "openmaptiles",
      "source-layer": "transportation",
      "filter": [
        "all",
        [
          "==",
          [
            "geometry-type"
          ],
          "LineString"
        ],
        [
          "match",
          [
            "get",
            "class"
          ],
          [
            "ferry",
            "aerialway"
          ],
          false,
          true
        ]
